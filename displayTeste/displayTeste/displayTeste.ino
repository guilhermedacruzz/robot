  if ( Display.getMode() == U8G_MODE_R3G3B2 ) {
    Display.setColorIndex(255);     //branco
  }
  else if ( Display.getMode() == U8G_MODE_GRAY2BIT ) {
    Display.setColorIndex(3);         //m√°xima intensidade
  }
  else if ( Display.getMode() == U8G_MODE_BW ) {
